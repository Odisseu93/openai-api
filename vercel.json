{
  "version": 2,
  "name": "OpenAi API",
  "github": {
    "enabled": true 
  },
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
      {
          "src": "(.*)",
          "dest": "src/app.ts",
          "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
          "headers": {
              "Access-Control-Allow-Origin": "*"
          }
      }
  ]
}
